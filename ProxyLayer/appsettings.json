{
  "Logging": {
    "LogLevel": {
      "Default": "Error"
    }
  },
  "AllowedHosts": "*",

  "AcmeSettingsLocation": "../../data/acmesettings.json",

  // Changes to the fields below will require an application restart
  "BypassRequestFilter": "false",

  "AllowEmptyUserAgent": "false",

  "UserAgents": {
    "Allowed": [
      "Firefox",
      "Chrome",
      "Edge",
      "Microsoft",
      "PostmanRuntime"
    ],
    "Blocked": [
      "Googlebot"
    ]
  },

  "Mappings": [
    //DeployManager
    {
      "Pattern": "/Build",
      "Port": "5999"
    },
    {
      "Pattern": "/Authenticate/",
      "Port": "6000"
    },
    {
      "Pattern": "/Factorio/",
      "Port": "6001"
    },
    {
      "Pattern": "/FactorioAcolyte",
      "Port": "6002"
    }
  ]
}
